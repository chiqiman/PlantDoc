{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": "off",
        "no-console": "off",
        "no-trailing-spaces": "off",
        "no-unused-vars": "off",
        "prefer-const": "off",
        "consistent-return": "off",
        "prefer-destructuring": "off"
    }
}
